{
  "name": "viseur",
  "version": "2.3.0",
  "description": "Visualizer for the Cadre AI game framework",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/siggame/Viseur"
  },
  "dependencies": {
    "@babel/core": "7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/plugin-proposal-optional-catch-binding": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@cadre/ts-utils": "2.0.0",
    "@types/chess.js": "0.10.1",
    "@types/color": "3.0.1",
    "@types/dateformat": "3.0.1",
    "@types/eases": "1.0.1",
    "@types/file-saver": "2.0.1",
    "@types/html-webpack-plugin": "3.2.3",
    "@types/jquery": "3.3.38",
    "@types/lodash": "4.14.153",
    "@types/node": "14.0.5",
    "@types/number-to-words": "1.2.0",
    "@types/seedrandom": "2.4.28",
    "@types/storejs": "2.0.3",
    "@types/webpack-env": "1.15.2",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "chess.js": "0.10.3",
    "color": "3.1.2",
    "core-js": "3.6.5",
    "css-loader": "3.5.3",
    "dateformat": "3.0.3",
    "eases": "1.0.8",
    "file-loader": "6.0.0",
    "file-saver": "2.0.2",
    "font-awesome": "4.7.0",
    "font-awesome-sass-loader": "2.0.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "handlebars": "4.7.6",
    "handlebars-loader": "1.7.1",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "jquery": "3.5.1",
    "less": "3.11.1",
    "lodash": "4.17.15",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "number-to-words": "1.2.4",
    "pixi.js": "5.2.4",
    "query-string": "6.12.1",
    "raw-loader": "4.0.1",
    "sass-loader": "8.0.2",
    "screenfull": "5.0.2",
    "seedrandom": "3.0.5",
    "store": "2.0.12",
    "style-loader": "1.2.1",
    "transform-loader": "0.2.4",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "ts-typed-events": "2.0.0",
    "typescript": "3.9.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0"
  },
  "devDependencies": {
    "@types/git-revision-webpack-plugin": "3.0.0",
    "@types/webpack": "4.41.13",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-jacobfischer": "github:JacobFischer/eslint-config.git#01336ff15c1d54bd3c21ab47c1999b77ec72939a",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-import-order-alphabetical": "1.0.0",
    "eslint-plugin-jsdoc": "26.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "git-revision-webpack-plugin": "3.0.6",
    "http-server": "0.12.3",
    "husky": "4.2.5",
    "npm-check-updates": "6.0.1",
    "prettier": "2.0.5",
    "shx": "0.3.2",
    "tslib": "2.0.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:raw": "webpack --progress --colors",
    "build:dev": "npm run build:raw -- --mode development",
    "build:prod": "npm run build:raw -- --mode production",
    "build:stats": "webpack --profile --json > webpack-stats.json",
    "clean": "npm run clean:build && npm run clean:docs && npm run clean:stats",
    "clean:docs": "shx rm -rf docs/",
    "clean:build": "shx rm -rf dist/",
    "clean:stats": "shx rm -rf webpack-stats.json",
    "analyze:stats": "webpack-bundle-analyzer webpack-stats.json",
    "analyze": "npm run build:stats && npm run analyze:stats",
    "docs": "typedoc --tsconfig ./tsconfig.json --out docs --module commonjs",
    "lint": "eslint ./ --max-warnings=0",
    "prettier:base": "prettier **/*.{js,ts}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:test": "npm run prettier:base -- --check",
    "js": "http-server dist/",
    "watch": "webpack-dev-server --progress --colors --host 0.0.0.0 --port 8080 --content-base dist/ --mode development",
    "start": "npm run watch",
    "test": "npm run lint && npm run build",
    "ncu": "ncu -u"
  }
}
